project Gem_Aes_Test is
    for Source_Dirs use ("src");
    for Object_Dir use "obj";
    for Main use ("main.adb");
    for Target use "m68k-atari-mint";
    package Builder is
        for Executable("main.adb") use "gemaes.prg";
    end Builder;
    package Compiler is
        for Default_Switches("Ada") use ("-O2", "-fomit-frame-pointer");
    end Compiler;
end Gem_Aes_Test;